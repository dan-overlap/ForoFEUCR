<%= form_for(presentation) do |f| %>
  <% if presentation.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(presentation.errors.count, "error") %> prohibited this presentation from being saved:</h2>

      <ul>
      <% presentation.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :authors_id %>
    <%= f.text_field :authors_id %>
  </div>

  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :abstract %>
    <%= f.text_field :abstract %>
  </div>

  <div class="field">
    <%= f.label :uploader_id %>
    <%= f.text_field :uploader_id %>
  </div>

  <div class="field">
    <%= f.label :pdf_document %>
    <%= f.file_field :pdf_document %>
    <%= f.submit "Submit" %>
  </div>

  <div class="field">
    <%= f.label :dislikes_id %>
    <%= f.text_field :dislikes_id %>
  </div>

  <div class="field">
    <%= f.label :status %>
    <%= f.number_field :status %>
  </div>

  <div class="field">
    <%= f.label :discussion_id %>
    <%= f.text_field :discussion_id %>
  </div>

  <div class="field">
    <%= f.label :presenters_id %>
    <%= f.text_field :presenters_id %>
  </div>

  <div class="field">
    <%= f.label :likes_id %>
    <%= f.text_field :likes_id %>
  </div>

  <div class="field">
    <%= f.label :dislikes_id %>
    <%= f.text_field :dislikes_id %>
  </div>
  

  <div class="actions">
